            <h2 class="choose">Choose <span>Order</span></h2>
            <div class="product-display">
            <% products.forEach(function(val){ %>
                
                <form action="/add-to-cart" method="POST">
                    <input type="hidden" name="id" value="<%= val.id%>">
                <div class="product">
                    <div class="p-img">
                        <img src="<%= val.photo %>" alt="">
                    </div>
                    <h3><%= val.name %></h3>
                    <h4><%= val.price%></h4>
                    <button class="add" type="submit" class="btn btn-primary">Add</button>
                </div>
                </form>
            <% }) %>
            </div>
        </div>
        <div class="right-cart">
            <div class="title-top df-ac-jsb">
                <h2 class="order">Order <span>Menu</span></h2>
                <!-- <i class="far fa-edit"></i> -->
            </div>
            <div class='Header'>
                <h3 class='Heading'>Items: <span style="font-size: 1.1vw;"><%= user.cart.items.length %></span></h3>
                <!-- <h5 class='Action'>Remove all</h5> -->
                <form action="/removeAllCart" method="POST">
                    <input type="hidden" name="prodId" value="">
                    <button style="font-weight: 600; outline: none; border: none; background: transparent;" type="submit" class="Action"><u>Remove All</u></button>
                </form>
            </div>
        
            <div class="order-list df-ac-jsb">                
                <div class='Cart-Container'>
                    <% for(let i = 0; i < user.cart.items.length; i++){
                        let eachItem = user.cart.items[i];  
                    %>
                <div class='Cart-Items df-ac'>
                    <div class="c-left df-ac">
                        <div class='image-box'>
                            <img src="<%= eachItem.productId.photo %>" alt="">
                        </div>
                        <div class='about'>
                            <h1 class='title'><%= eachItem.productId.name %></h1>
                            <h3 class='subtitle'> <%= eachItem.productId.price %></h3>
                        </div>
                    </div>
                    <div class="c-right df-ac">
                        <div class='counter df-ac'>
                            <div class='btn plus'>+</div>
                            <input type="text" class="quantity-input count" style="width: 30px; opacity: .8; color: #fff; border: 1px solid white; border-radius: 50px; background-color: #FF0F5B; text-align: center;" value="<%= eachItem.qty %>">
                            <!-- <div class='count'><h3 style="opacity: .8; color: #fff; border: 1px solid white; border-radius: 50px; background-color: #FF0F5B; text-align: center;"><%= eachItem.qty %></h3></div> -->
                           <div class='btn minus'>-</div>
                        </div>
                        <div class='prices df-ac-jsb'>
                            <div class='amount'><%= eachItem.productId.price * eachItem.qty %></div>
                            <form action="/deleteInCart" method="POST">
                                <input type="hidden" name="prodId" value="<%= eachItem.productId._id %>">
                                <button style="outline: none; border: none; background: transparent;" type="submit" class='remove'><i class="fas fa-trash-alt"></i></button>
                            </form>
                        </div>
                    </div>
                </div>
                <%}%>
            </div>
            <br>
        </div>
        <hr>
            <div class='checkout'>
                <div class='total'>
                    <div>
                        <div class='Subtotal'>Sub-Total</div>
                        <div class='Subtotal text-lite'>Tax 5%</div>
                        <!-- <div class='Subtotal text-lite'>Parcel Charge</div> -->
                    </div>
                    <div class="t-amt">
                        <div class='total-amount'> <%= parseFloat(user.cart.totalPrice).toFixed(2) %></div>
                        <div class='total-amount text-lite'> <%= parseFloat(user.cart.totalPrice * .05).toFixed(2) %></div>
                        <!-- <div class='total-amount text-lite'> 10</div> -->
                    </div>
                </div>
            </div>
            <a href="#"><button class='charge'><h2>Checkout</h2><h3> <%= parseFloat(user.cart.totalPrice + (user.cart.totalPrice * .05)).toFixed(0) %></h3></button></a>
            <form action="">
                
            </form>
    </div>
